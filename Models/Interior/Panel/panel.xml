    <object-name>panel</object-name>

    <!-- Objets transparents -->
  </animation>

  <!-- ALT -->
  <model>
    <path>Aircraft/Curtiss-Model-F/Models/Interior/Panel/Instruments/alt/alt.xml</path>
    <offsets>
      <x-m>  0.009 </x-m>
      <y-m> -0.140 </y-m>
      <z-m>  0.050 </z-m>
    </offsets>
  </model>
 
  <!-- ASI -->
  <model>
    <path>Aircraft/Curtiss-Model-F/Models/Interior/Panel/Instruments/asi/asi.xml</path>
